<h1>Registered Websites</h1>
<div class="row">
  <div class="col-md-8">
    <%= @registered_applications.each do |app| %>
    <div class="media">
      <div class="media-body">
        <h4 class="media-heading">
          <%= link_to @registered_application.name, @registered_application %>
        </h4>
        <small>
          <%= @registered_application.url %>
        </small>
      </div>
    </div>
    <% end %>
  </div>
</div>
